<!doctype html>
<html lang="ru">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    {{ fn('get_scss') }}
    {{ fn('wp_head') }}
    {{ fn('wp_footer') }}
    {{ fn('get_js') }}
</head>
<body {{ fn('body_class') }}>
<div class="wrap">
    <header>
        <div class="desktop-header">
            <div class="container">
                <a href="#" class="logo_virna">
                    <svg class="icon">
                        <use xlink:href="/wp-content/themes/site/images/svg/sprite.svg#virna"></use>
                    </svg>
                </a>
                <a href="#" class="logo_bonum">
                    <svg class="icon">
                        <use xlink:href="/wp-content/themes/site/images/svg/sprite.svg#bonum"></use>
                    </svg>
                </a>
            </div>
        </div>
    </header>
    {% block content %}  {% endblock %}
</div>
<footer>
    <div class="container">

    </div>
</footer>
<div class="to-top"></div>
</body>
</html>